    "version": "0.2.0",
    "configurations": [        
        {
            "type": "node",
            "name":"launch protractor test",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "outputCapture": "std",
            // "stopOnEntry": false,
            // "sourceMaps": true,
            // "smartStep": true,
            // "cwd": "${workspaceRoot}",
            "args": ["--troubleshoot=true", "${workspaceRoot}/protractor.conf.debug.js"],
            // "preLaunchTask": "startWebDriver",
            // "postDebugTask": "stopWebdriver"
        },
        {
            "type": "node",
            "name":"get snippets",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "outputCapture": "std",
            // "stopOnEntry": false,
            // "sourceMaps": true,
            // "smartStep": true,
            // "cwd": "${workspaceRoot}",
            "args": ["--troubleshoot=true", "${workspaceRoot}/protractor.conf.debug.js"],
            // "preLaunchTask": "startWebDriver",
            // "postDebugTask": "stopWebdriver"
        },
        {
            "type": "node",
            "name":"get snippets old",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/cucumber/bin/cucumber-js",
            "outputCapture": "std",
            // "stopOnEntry": false,
            // "sourceMaps": true,
            // "smartStep": true,
            // "cwd": "${workspaceRoot}",
            "args": ["--format-options '{'snippetInterface': 'async-await'}'", "${workspaceRoot}/features/RegisterUsers.feature"],
            // "preLaunchTask": "startWebDriver",
            // "postDebugTask": "stopWebdriver"
        }
    ]
}